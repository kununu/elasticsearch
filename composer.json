{
    "name": "kununu/elasticsearch",
    "description": "Library for simplified querying and usage of Elasticsearch @ Kununu",
    "type": "library",
    "license": "proprietary",
    "minimum-stability": "stable",
    "require": {
        "php": ">=8.0",
        "ext-ctype": "*",
        "ext-iconv": "*",
        "ext-json": "*",
        "ext-mbstring": "*",
        "elasticsearch/elasticsearch": "^7.0.0",
        "psr/log": "~1.0"
    },
    "require-dev": {
        "mockery/mockery": "^1.4",
        "phpunit/phpunit": "^9.5"
    },
    "config": {
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "Kununu\\Elasticsearch\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Kununu\\Elasticsearch\\Tests\\": "tests/"
        }
    }
}
